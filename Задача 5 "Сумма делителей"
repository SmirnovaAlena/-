#include <iostream>
#include <vector>

using namespace std;


int main() {
    int n;
    cin >> n;
    vector<int> dp(n+1 , 1e9);
    dp[1] = 1;
    for (int i = 2; i <= n; i++) {
        if (i % 2 == 0) {
            dp[i] = min(dp[i], dp[i / 2] + 1);
            
            
        }
        if (i % 3 == 0) {
            dp[i] = min(dp[i], dp[i / 3] + 1);
            
        }

    }

    cout<< dp[n-1];

    return 0;
}
