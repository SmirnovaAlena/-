#include <iostream>
#include <vector>
using namespace std;
int func(vector<vector<int>>& v) {
    int n = v.size();
    int m = v[0].size();
    if (v[0][0] == 1) {
        return 0;
    }





    vector<vector<int>> dp(n, vector<int>(m, 0));
    dp[0][0] = 1;
    for (int j = 1; j < m; j++) {
        if (v[0][j] == 0) {
            dp[0][j] = dp[0][j - 1];
        }
        else {
            dp[0][j] = 0;
        }
    }
    for (int i = 1; i < n; i++) {
        if (v[i][0] == 0) {
            dp[i][0] = dp[i - 1][0];
        }
        else {
            dp[i][0] = 0;
        }
    }
    for (int i = 1; i < n; i++) {
        for (int j = 1; j < m; j++) {
            if (v[i][j] == 0) {
                dp[i][j] = dp[i - 1][j] + dp[i][j - 1];
            }
            else {
                dp[i][j] = 0;
            }
        }
    }
    return dp[n - 1][m - 1];
}
int main() {
    vector<vector<int>> v = {{0, 1, 0},
        {0, 1, 0},
        {0, 0, 0}
    };
    cout << func(v);
    return 0;
}
